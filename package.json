{
  "name": "hi-client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:17628",
  "dependencies": {
    "koa": "^2.3.0",
    "koa-json": "^2.0.2",
    "koa-router": "^7.2.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13"
  },
  "scripts": {
    "start-server": "supervisor app.js",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
